/* pages/myroom/myroom.wxss */
.container1 {
  max-height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
} 
.re,.head{
    float: left;
}
.re{
    width: 16.59375vw;
    height: 13.125vw;
    margin: 49px 0 0 11.875%;
    text-align: center;
}
.resource {
    width: 8.125vw;
    height: 8.125vw;
    margin: 0 0 5px 15px;

}
.resource image{
    width: 100%;
    height: 100%;
    background-size: cover;

}
.resource_add{
    font-size: 3.75vw;
    font-weight: 600;
    color: #fff;
 
}
/*.word{
    margin-left: 7px;
}*/
.head{
 
    margin-top: 30px;
    margin-left: 38px;
    width: 25vw;
    height: 25vw;
    border: 2px solid #fff;
    border-radius: 100%;
    
}
.headimg{
    width: 100%;
    height: 100%;
    border-radius: 100%;
    background-size: cover;
}
.headname{
   width: 100vw;
   font-size: 4.6875vw;
   color: #fff;
   font-weight: 600;
   justify-content:center;
   padding-top: 15px;
   display: flex;
   margin:20rpx;

}
.headcity{
    font-size: 4.0625vw;
    color:#fff;
    font-weight: 600;
    text-align:center;
    margin: 5px 0 15p 0;
}
.headinfo{
    margin: 20rpx 20rpx ;
    border-top:1px solid #7B7B7B;
    border-bottom:1px solid #eee;
    display: flex;
}
.add-resou{
    height: 25px;
    line-height:25px;   
     overflow:hidden;  
    margin-left: 170px;
    font-size: 14px;
    border: 2px solid #eee;
    margin-top: 5px;
    margin-bottom: 5px;
    border-radius:25%;

}
.headtext{
    margin-top: 20rpx; 
    font-size: 15px;
}


.user-top{
 width: 100vw;
 overflow: hidden; 
 background: #13b1a3;
}

.na_word{
    overflow: hidden;
    clear: both;
    width:16.59375vw;
     height:13.125vw;
     margin:49px 0 0 11.875%;

}
