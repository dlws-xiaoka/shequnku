
<!--pages/dataone/dataone.wxml-->
<view class="dataone_container">

  <!--社团名称以及省市部分-->
  <view class="top"></view>
  <view class="content_wrapper">
    <view class="content">
      <view class="content_detail">
        <text class="content_name1">类别选择</text>
        <picker class="content_nameP" bindchange="changeUserType" value="{{typeIndex}}" range="{{userTypeArray}}">
          <view class="picker_choose" id="{{TypeIdArray}}">
            {{userTypeArray[typeIndex]}}
          </view>
        </picker>
        <view class="content_pic">
          <image src="/image/more.png" class="photo"></image>
        </view>
      </view>
      <block wx:if="{{typeIndex==0}}">
        <view class="content_detail">
          <text class="content_name1">社团名称</text>
          <input class="input content_name2" name="userName" placeholder="请输入社团名称" bindinput="userNameInput" />
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">选择省</text>
          <picker class="content_nameP"  bindchange="changeProven" value="{{ProvenIndex}}" range="{{provinceList}}">
            <view class="picker_text">
            {{provinceList[ProvenIndex]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">选择市</text>
          <picker class="content_nameP" bindchange="changeCity" value="{{cityIndex}}" range="{{cityList}}">
            <view class="picker_text">
               {{cityList[cityIndex]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">院校名称</text>

          <!--<input class="content_nameCity" placeholder="北京清华大学"/>-->
          <picker class="content_nameCity" bindchange="chooseSchool" value="{{cityIndex}}" range="{{schoolList}}">
            <view class="picker_choose">
              {{schoolList[cityIndex]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
      </block>

      <block wx:if="{{typeIndex==1}}">
        <view class="content_detail">
          <text class="content_name1">微信号</text>
          <input class="input content_W" name="userName" placeholder="请输入微信号" bindinput="userNameInput" />
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">微信名称</text>
          <input class="input content_W" name="userName" placeholder="请输入微信名称" bindinput="userNameInput" />
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">微信类别</text>
          <picker class="content_nameP" bindchange="wxChoose" value="{{locationIndex5}}" range="{{locationArray5}}">
            <view class="picker_wx">
              55555 {{locationArray5[locationIndex5]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
      </block>


      <block wx:if="{{typeIndex==2}}">
        <view class="content_detail">
          <text class="content_name1">微信群名称</text>
          <input class="input content_W" name="userName" placeholder="请输入微信群名称" bindinput="userNameInput" />
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">微信群类别</text>
          <picker class="content_nameP" bindchange="wxChoose" value="{{locationIndex5}}" range="{{locationArray5}}">
            <view class="picker_wx">
              55555 {{locationArray5[locationIndex5]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
      </block>
      <block wx:if="{{typeIndex==3}}">
        <view class="content_detail">
          <text class="content_name1">QQ群名称</text>
          <input class="input content_Q" name="userName" placeholder="请输入QQ群名称" bindinput="userNameInput" />
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
        <view class="content_detail">
          <text class="content_name1">QQ群类别</text>
          <picker class="content_nameP" bindchange="wxChoose" value="{{locationIndex5}}" range="{{locationArray5}}">
            <view class="picker_wx">
              55555 {{locationArray5[locationIndex5]}}
            </view>
          </picker>
          <view class="content_pic">
            <image src="/image/more.png" class="photo"></image>
          </view>
        </view>
      </block>

      <!-- <view class="content_detail">
            <text class="content_name1">公众号名称</text>
            <input class="input content_G" name="userName" placeholder="请输入公众号名称"
             bindinput="userNameInput"/>
            <view class="content_pic">
              <image src="/image/more.png" class="photo"></image>
            </view>
        </view>-->
      <view class="content_detail">
        <text class="content_name1">手机号</text>
        <input class="content_nameT" placeholder="请输入手机号" />
        <view class="content_pic">
          <image src="/image/more.png" class="photo"></image>
        </view>
      </view>
      <view class="content_detail">
        <text class="content_name1">简介</text>
        <textarea class="content_nameJ" placeholder="请输入简介" />
        <view class="content_pic">
          <image src="/image/more.png" class="photo"></image>
        </view>
      </view>
    </view>
  </view>
  <view class="sorting">
    <view class="title">
      <text>一级分类</text>
      <view class="sort_pic">
        <image class="photo" src="/image/down.png"></image>
      </view>
    </view>
    <block wx:if="{{true}}" class="genre_container">
      <view class="genre" bindtap="changeColor">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二三级分类</view>
      <view class="genre">二级分类</view>
    </block>
  </view>
  <view class="sorting">
    <view class="title">
      <text>一级分类</text>
      <view class="sort_pic">
        <image class="photo" src="/image/down.png"></image>
      </view>
    </view>
    <block wx:if="{{true}}" class="genre_container">
      <view class="genre" bindtap="changeColor">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二三级分类</view>
      <view class="genre">二级分类</view>
    </block>
  </view>
  <view class="sorting">
    <view class="title">
      <text>一级分类</text>
      <view class="sort_pic">
        <image class="photo" src="/image/down.png"></image>
      </view>
    </view>
    <block wx:if="{{true}}" class="genre_container">
      <view class="genre" bindtap="changeColor">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二级分类</view>
      <view class="genre">二三级分类</view>
      <view class="genre">二级分类</view>
    </block>
  </view>
</view>
<button class="foot_button" type="default" size="{{defaultSize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="default" hover-class="other-button-hover">完成 </button>