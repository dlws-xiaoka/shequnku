/* pages/searchList/searchList.wxss */
@import "../wxSearch/wxSearch.wxss"
.important_search{
    width: 100vw;
    overflow: hidden;
    clear: both;
}
.search_navigator{
    overflow: hidden;
    line-height: 0;
}
.item{
    overflow: hidden;
     border-bottom: 1px solid #ccc;
    display: flex;
    flex-flow: nowrap row;
    justify-content: space-between;
}
/*text{
    font-size: 4.0375vw;
    color:#333;
    font-weight: 500;
    margin: 15px 0 15px 10px;
}*/
.important_pic{
    width: 4.375vw;
    height: 4.375vw;
    background-size: cover;
    margin: 15px 10px 15px 0;
}
.article__title{
    height: 4.6875vw;
    line-height: 4.6875vw;
}

/*取消button默认样式*/
button[size="mini"] {
display:inline-block;
line-height:0;
font-size:0;
padding:0;

}
button[plain] {
color:#fff;
border:0;
background-color:#fff;
}
button {
display:block;
margin-left:0;
margin-right:0;
padding-left:0;
padding-right:0;
box-sizing:border-box;
font-size:0;
text-align:center;
text-decoration:none;
line-height:0;
border-radius:0;
-webkit-tap-highlight-color:#fff;
overflow:hidden;
color:#fff;
background-color:#fff;

}
.search_img1{
    width: 100%;
    height: 100%;
}
button::after{
    border: 0;
}
